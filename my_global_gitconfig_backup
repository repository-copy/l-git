## Копия глобального файла /home/tolik/.gitconfig
[user]
	email = tolik.demianov@gmail.com
	name = Tolik Dem
[alias]
	ch = checkout
	co = commit -m
	st = status
	br = branch
	hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=format-local:'%F %T'
        hist2 = log --pretty=format:'%H %ad | %s%d [%an]' --graph --date=format-local:'%F %T'
[core]
	pager = less -RFX
	excludesFile = /home/tolik/.gitignore
	autocrlf = input
